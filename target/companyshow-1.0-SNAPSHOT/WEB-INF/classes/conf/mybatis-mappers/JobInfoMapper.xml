<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solidtime.companyshow.dao.JobInfoDao">

    <select id="getJobInfoById" resultType="com.solidtime.companyshow.bean.JobInfo">
        SELECT * FROM jobinfo WHERE id=#{id}
    </select>

    <select id="getJobInfos" resultType="com.solidtime.companyshow.bean.JobInfo">
        SELECT * FROM jobinfo
    </select>
</mapper>